<div style="height: <%= data[:div_height] %>; width: <%= data[:div_width] %>;">
	<table style="width:100%; height: 96%; border: <%= data[:border_size] %>px solid black; font-family: monospace; justify-content: center; align-items: center;">
		<% data[:items].each_with_index do |btn, ix| %>
		<tr>
			<td style="border: 1px solid black;  font-size: <%= data[:font_size][ix] %>px; text-align: <%= data[:text_align][ix] %>; padding-left: 5%; margin-right: -5%;"><%= btn %></td>
		</tr>
		<% end %>
		<% (data[:rows] - data[:items].count).times do |ix| %>
		<tr>
			<td style="border: 1px solid black;  font-size: <%= data[:font_size][ix] %>px; text-align: <%= data[:text_align][ix] %>; padding-left: 5%; margin-right: -5%;">-</td>
		</tr>
		<% end %>
	</table>
</div>