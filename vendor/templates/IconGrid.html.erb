<div style="height: <%= data[:div_height] %>; width: <%= data[:div_width] %>; display: flex; flex-flow: row; align-items: center;">
	<div class="icons" style="width: 100%; display: flex; flex-flow: row; justify-content: space-around; flex-wrap: wrap;">
		<% data[:grid_items].each do |icn| %>
			<div style="width: <%= data[:icon_size] %>px; height: <%= data[:icon_size] %>px; display:flex; justify-content:center; align-items:center;">
				<%= icn %>
			</div>
		<% end %>
	</div>
</div>